{
	"Create readme": {
		"prefix": "#readme",
		"body": [
			"# $1",
			"<!-- short description of package -->",
			"## About",
			"<!-- long description of package -->",
			"## Installation",
			"<!-- installation instructions including prerequisites -->",
			"## Usage",
			"<!-- description of setup, command options, etc -->",
			"## License",
			"<!-- link to license file -->",
			"## Contributing",
			"<!-- link to contributing instructions -->"
		],
		"description": "Outline for README file",
	},
	"Insert Front Matter": {
		"prefix": "frontmatter",
		"body": [
			"---",
			"layout: text_entry",
			"title: ${1:title}",
			"category: ${2:Robotics}",
			"series: ${3}",
			"tags: ${4}",
			"image: ${5}",
			"summary: ${6}",
			"redirect_from: ",
			"---"
		]
	}
}
