{
	"dockerFile": "Dockerfile",
	"runArgs": [
		"--cap-add=SYS_PTRACE", 
		"--security-opt", "seccomp=unconfined"
	],
	"extensions": [
		"ms-azuretools.vscode-docker",
		"ms-python.python",
		"ms-vscode.cpptools",
		"twxs.cmake"
	]
}