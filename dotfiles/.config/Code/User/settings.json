{
    "[cpp]": {
        "editor.rulers": [
            80
        ],
        "editor.tabSize": 2
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker",
        "editor.quickSuggestions": {
            "strings": true
        }
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 80
    },
    "[python]": {
        "editor.tabSize": 4
    },
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "codeium.enableConfig": {
        "*": true,
        "starlark": true
    },
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "settings.json"
    ],
    "cSpell.userWords": [
        "athackst"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "dotfiles.repository": "https://github.com/athackst/workstation_setup.git",
    "editor.bracketPairColorization.enabled": true,
    "editor.inlineSuggest.enabled": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "alt",
    "editor.rulers": [
        80,
        100
    ],
    "files.associations": {
        ".rosinstall": "yaml",
        "*.bats": "shellscript",
        "*.xacro": "xml"
    },
    "files.autoSave": "afterDelay",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "githubPullRequests.defaultMergeMethod": "squash",
    "githubPullRequests.quickDiff": true,
    "gitlens.views.scm.grouped.views": {
        "branches": false,
        "commits": true,
        "contributors": true,
        "launchpad": false,
        "remotes": true,
        "repositories": true,
        "searchAndCompare": false,
        "stashes": false,
        "tags": true,
        "worktrees": true
    },
    "grunt.autoDetect": "off",
    "gulp.autoDetect": "off",
    "jake.autoDetect": "off",
    "npm.autoDetect": "off",
    "redhat.telemetry.enabled": false,
    "remote.autoForwardPortsSource": "hybrid",
    "settingsSync.ignoredExtensions": [
        "shopify.ruby-lsp",
        "codeium.codeium"
    ],
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.tabs.enabled": true,
    "typescript.tsc.autoDetect": "off",
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editorAssociations": {
        "filenamePattern": "*.ipynb",
        "viewType": "jupyter.notebook.ipynb"
    },
    "yaml.schemas": {
        "file:///home/vscode/.vscode-server/extensions/docsmsft.docs-yaml-1.0.5/dist/toc.schema.json": "/toc\\.yml/i",
        "http://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/dependabot-2.0.json": [
            "dependabot.yml"
        ],
        "http://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/github-workflow.json": [
            "/.github/workflows/*"
        ]
    }
}
